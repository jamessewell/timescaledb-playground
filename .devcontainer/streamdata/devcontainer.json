{
  "name": "timescaledb-tutorial",
  "dockerComposeFile": "./compose.yaml",
  "service": "timescaledb",
  "runServices": ["shadowtraffic", "timescaledb"],
  "postCreateCommand": {
     "database": "psql -f ./streamdata/init.sql"
  },
  "postAttachCommand": "psql",
  "customizations": {
      "codespaces": {
          "openFiles": [
              "./sql/welcome.sql"
          ]
      }
  }
}
